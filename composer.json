{
    "name": "erichard/dms-bundle",
    "description": "A Document Management System for Symfony 2",
    "type": "symfony-bundle",
    "autoload": {
        "psr-0": { "Erichard\\DmsBundle": "" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.9.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "symfony/symfony": ">=2.1,<3.0",
        "braincrafted/bootstrap-bundle": "dev-master",
        "twitter/bootstrap": "2.3.*",
        "jquery/jquery": "1.9.*",
        "leafo/lessphp": "0.3.*",
        "stof/doctrine-extensions-bundle": "dev-master"
    },
    "require-dev": {
        "nelmio/alice": "dev-master",
        "phpspec/phpspec2": "dev-develop",
        "phpspec/php-diff": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master"
    },
    "minimum-stability": "alpha",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
